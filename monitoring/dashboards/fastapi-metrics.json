{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0,211,255,1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "iteration": 2,
  "panels": [
    {
      "id": 1,
      "type": "graph",
      "title": "HTTP Request Count (per 5m)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 },
      "targets": [
        { "refId": "A", "expr": "sum(rate(http_requests_total[5m])) by (method, endpoint, status_code)", "legendFormat": "{{method}} {{endpoint}} {{status_code}}" }
      ],
      "lines": true, "linewidth": 2, "fill": 1
    },
    {
      "id": 2,
      "type": "graph",
      "title": "Avg HTTP Request Duration (sec)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 6 },
      "targets": [
        { "refId": "A", "expr": "sum(rate(http_request_duration_seconds_sum[5m])) by (method, endpoint) / sum(rate(http_request_duration_seconds_count[5m])) by (method, endpoint)", "legendFormat": "{{method}} {{endpoint}}" }
      ],
      "lines": true, "linewidth": 2, "fill": 1,
      "yaxis": { "format": "s", "label": "Seconds" }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "CPU Usage (%)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "x": 0, "y": 6, "w": 6, "h": 4 },
      "targets": [ { "refId": "A", "expr": "process_cpu_percent" } ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Thread Count",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "x": 6, "y": 6, "w": 6, "h": 4 },
      "targets": [ { "refId": "A", "expr": "process_thread_count" } ]
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Open File Descriptors",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "x": 12, "y": 6, "w": 6, "h": 4 },
      "targets": [ { "refId": "A", "expr": "process_open_fds" } ]
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Process Uptime (sec)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "x": 18, "y": 6, "w": 6, "h": 4 },
      "targets": [ { "refId": "A", "expr": "process_uptime_seconds" } ]
    },
    {
      "id": 7,
      "type": "graph",
      "title": "Memory Usage (bytes)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "x": 0, "y": 10, "w": 12, "h": 6 },
      "targets": [
        { "refId": "A", "expr": "process_resident_memory_bytes", "legendFormat": "Resident" },
        { "refId": "B", "expr": "process_virtual_memory_bytes", "legendFormat": "Virtual" }
      ],
      "lines": true, "linewidth": 2, "fill": 1, "format": "bytes"
    },
    {
      "id": 8,
      "type": "graph",
      "title": "GC Collections by Generation",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "x": 12, "y": 10, "w": 12, "h": 6 },
      "targets": [
        { "refId": "A", "expr": "python_gc_collections_total{generation=\"0\"}", "legendFormat": "Gen0" },
        { "refId": "B", "expr": "python_gc_collections_total{generation=\"1\"}", "legendFormat": "Gen1" },
        { "refId": "C", "expr": "python_gc_collections_total{generation=\"2\"}", "legendFormat": "Gen2" }
      ],
      "lines": true, "linewidth": 2, "fill": 1
    },
    {
      "id": 9,
      "type": "graph",
      "title": "GC Objects Collected by Generation",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 6 },
      "targets": [
        { "refId": "A", "expr": "python_gc_objects_collected_total{generation=\"0\"}", "legendFormat": "Gen0" },
        { "refId": "B", "expr": "python_gc_objects_collected_total{generation=\"1\"}", "legendFormat": "Gen1" },
        { "refId": "C", "expr": "python_gc_objects_collected_total{generation=\"2\"}", "legendFormat": "Gen2" }
      ],
      "lines": true, "linewidth": 2, "fill": 1
    }
  ],
  "refresh": "15s",
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["python", "prometheus", "process", "http", "metrics", "gc", "memory"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "label": "Prometheus",
        "refresh": 1
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "15s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "timezone": "browser",
  "title": "FastAPI Metrics (Shared)",
  "version": 1
}
